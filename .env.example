# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_auth_domain
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_storage_bucket
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Admin Password
REACT_APP_ADMIN_PASSWORD=your_admin_password

# EmailJS Configuration (https://www.emailjs.com/)
# 1. Đăng ký tài khoản tại https://www.emailjs.com/
# 2. Tạo Email Service (Gmail)
# 3. Tạo Email Template với các biến: {{to_email}}, {{employee_name}}, {{shifts_list}}, {{shifts_count}}, {{registration_time}}
# 4. Lấy Service ID, Template ID, Public Key
REACT_APP_EMAILJS_SERVICE_ID=your_service_id
REACT_APP_EMAILJS_REGISTRATION_TEMPLATE_ID=template_registration_id
REACT_APP_EMAILJS_SCHEDULE_TEMPLATE_ID=template_hubsnbi
REACT_APP_EMAILJS_PUBLIC_KEY=your_public_key

# Admin Email (nhận thông báo khi có đăng ký mới)
REACT_APP_ADMIN_EMAIL=admin@example.com
